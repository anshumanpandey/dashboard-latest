@model List<NHS.Common.DTO.clsCOVIDDetails>
@using NHS.Models;
@{
    ViewBag.Title = "COVID 19 Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
    <style>
        #Newpatient {
            font-size: 13px;
        }

            #Newpatient:hover {
                text-decoration: underline;
                color: cornflowerblue !important;
            }

        #DOD_ToolTip {
            display: none;
            position: absolute;
            border: 1px solid gray;
            WIDTH: 254PX;
            HEIGHT: 100PX;
            Z-INDEX: 999999;
            background-color: white;
        }

        #DRow {
            padding-right: 1px !Important;
        }

        #DateOfDeath:hover {
            cursor: context-menu;
        }
        /*.tooltip {
            border:1px solid gray;
            padding:5px !important;
        }*/
        #DateOfDeath.tooltip {
            border: 1px solid gray;
            padding: 5px !important;
        }
    </style>
}




<br />
<br />
@*<script src="cdnjs.cloudflare.com/ajax/libs/moment.js/2.8.4/moment.min.js"></script>*@

<!-- DataTables -->

<div>
    <table width="100%" border="0" cellspacing="0" cellpadding="0">
        <tr>
            <td width="40%">
                <div id="showfilterdiv" class="ds_filter_btn" style="display:none !important;">
                    <a href="javascript:void(0);" onclick="ShowFilter()" id="showfilter" @*class="ds_filter_btn_show"*@> <img src="~/images/show-arrow.png" alt="" />Show Advanced Filters</a>
                </div>
                <div id="hidefilterdiv" class="ds_filter_btn" style="display:block !important;">
                    <a href="javascript:void(0);" onclick="HideFilter()" id="hidefilter" @*class="ds_filter_btn_hide"*@> <img src="~/images/hide-arrow.png" alt="" />Hide Advanced Filters</a>
                </div>
            </td>
            <td width="60%" align="right" style="padding:10px 15px"><b>Data Refreshed: From @ViewBag.UpdatedSource at @ViewBag.UpdatedDate @ViewBag.UpdatedTime</b></td>
        </tr>

    </table>
</div>
<div class="col-sm-12">
    <div class="row">
        <div class="col-md-12">
            <form action="@Url.Action("COVIDDashboard", "Home", new { buttonsubmit = true })" method="post">
                <div class="row" id="filtercondition">
                    <div class="col-md-12">
                        <div class="row">
                            <div class="col-md-3">
                                <div class="row_bar">
                                    <input style="font-size:10px;height:38px; margin:0 0 0 0; width:100%" class="form-control  datepicker" readonly="readonly" type="text" id="txtStartDate" name="txtStartDate" placeholder="All Dates" value="Order Start Date @DateTime.ParseExact(Session["COVIDStartDate"].ToString(), "dd/MM/yyyy", System.Globalization.CultureInfo.InvariantCulture).ToString("dd/MM/yyyy")">
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="row_bar">
                                    <input style="font-size:10px; margin:0 0 0 0; height:38px;width:100%" class="form-control  datepicker" readonly="readonly" type="text" id="txtEndDate" name="txtEndDate" placeholder="All Dates" value="Order End Date @DateTime.ParseExact(Session["COVIDEndDate"].ToString(), "dd/MM/yyyy", System.Globalization.CultureInfo.InvariantCulture).ToString("dd/MM/yyyy")">&nbsp;&nbsp;
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="row_bar">
                                    <select style="font-size:12px;  margin:0 0 0 0;" class="form-control" id="ddlAdmissionStatus" name="ddlAdmissionStatus">
                                        @if (Convert.ToString(Session["TestAdmissionStatus"]) == "0")
                                        {
                                            <option selected value="0">
                                                All Admission Status
                                            </option>
                                        }
                                        else
                                        {
                                            <option value="0">
                                                All  Admission Status
                                            </option>
                                        }
                                        @foreach (var item in ViewBag.AdmissionStatus)
                                        {
                                            if (!string.IsNullOrEmpty(item.AdmissionStatus))
                                            {
                                                if (item.AdmissionStatus == Convert.ToString(Session["TestAdmissionStatus"]) && Convert.ToString(Session["AdmissionStatus"]) != "0"
                                                    && Convert.ToString(Session["TestAdmissionStatus"]) != "")
                                                {
                                                    <option selected value="@item.AdmissionStatus">@item.AdmissionStatus</option>
                                                }
                                                else
                                                {
                                                    <option value="@item.AdmissionStatus">@item.AdmissionStatus</option>
                                                }
                                            }
                                        }
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="row_bar">
                                    <select style="font-size:12px;  margin:0 0 0 0;" class="form-control" id="ddlNotification" name="ddlNotification">
                                        @if (Convert.ToString(Session["TestNotification"]) == "0")
                                        {
                                            <option selected value="0">
                                                All Notification
                                            </option>
                                        }
                                        else
                                        {
                                            <option value="0">
                                                All Notification
                                            </option>
                                        }
                                        @foreach (var item in ViewBag.Notification)
                                        {
                                            if (item.Notification == Convert.ToString(Session["TestNotification"]) && Convert.ToString(Session["Notification"]) != "0"
                                                && Convert.ToString(Session["TestNotification"]) != "")
                                            {
                                                <option selected value="@item.Notification">@item.Notification</option>
                                            }
                                            else
                                            {
                                                <option value="@item.Notification">@item.Notification</option>
                                            }
                                        }
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="row_bar">
                                    <select style="font-size:12px;  margin:0 0 0 0;" class="form-control" id="ddlTestStatus" name="ddlTestStatus">
                                        @if (Convert.ToString(Session["TestTestStatus"]) == "0")
                                        {
                                            <option selected value="0">
                                                All Order Status
                                            </option>
                                        }
                                        else
                                        {
                                            <option value="0">
                                                All  Order Status
                                            </option>
                                        }
                                        @foreach (var item in ViewBag.LoadTestStatus)
                                        {
                                            if (!string.IsNullOrEmpty(item.TestStatus))
                                            {
                                                if (item.TestStatus == Convert.ToString(Session["TestTestStatus"]) && Convert.ToString(Session["TestStatus"]) != "0"
                                                    && Convert.ToString(Session["TestTestStatus"]) != "")
                                                {
                                                    <option selected value="@item.TestStatus">@item.TestStatus</option>
                                                }
                                                else
                                                {
                                                    <option value="@item.TestStatus">@item.TestStatus</option>
                                                }
                                            }
                                        }
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="row_bar">
                                    <select style="font-size:12px;  margin:0 0 0 0;" class="form-control" id="ddlTestResults" name="ddlTestResults">
                                        @if (Convert.ToString(Session["TestTestResults"]) == "0")
                                        {
                                            <option selected value="0">
                                                All Test Results
                                            </option>
                                        }
                                        else
                                        {
                                            <option value="0">
                                                All Test Results
                                            </option>
                                        }
                                        @foreach (var item in ViewBag.TestResults)
                                        {
                                            if (!string.IsNullOrEmpty(item.TestResult))
                                            {
                                                if (item.TestResult == Convert.ToString(Session["TestTestResults"]) && Convert.ToString(Session["TestResults"]) != "0"
                                                    && Convert.ToString(Session["TestTestResults"]) != "")
                                                {
                                                    <option selected value="@item.TestResult">@item.TestResult</option>
                                                }
                                                else
                                                {
                                                    <option value="@item.TestResult">@item.TestResult</option>
                                                }
                                            }
                                        }
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="row_bar">
                                    <select style="font-size:12px;  margin:0 0 0 0;" class="form-control" id="ddlTestOrderLocation" name="ddlTestOrderLocation">
                                        @if (Convert.ToString(Session["TestTestOrderLocation"]) == "0")
                                        {
                                            <option selected value="0">
                                                All Test Order Location
                                            </option>
                                        }
                                        else
                                        {
                                            <option value="0">
                                                All  Test Order Location
                                            </option>
                                        }
                                        @foreach (var item in ViewBag.TestOrderLocation)
                                        {
                                            if (!string.IsNullOrEmpty(item.TestOrderLocation))
                                            {
                                                if (item.TestOrderLocation == Convert.ToString(Session["TestTestOrderLocation"]) && Convert.ToString(Session["TestOrderLocation"]) != "0"
                                                    && Convert.ToString(Session["TestTestOrderLocation"]) != "")
                                                {
                                                    <option selected value="@item.TestOrderLocation">@item.TestOrderLocation</option>
                                                }
                                                else
                                                {
                                                    <option value="@item.TestOrderLocation">@item.TestOrderLocation</option>
                                                }
                                            }
                                        }
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-2">
                                <div class="row_bar">
                                    <select style="font-size:12px;  margin:0 0 0 0;" class="form-control" id="ddlLastLocation" name="ddlLastLocation">
                                        @if (Convert.ToString(Session["TestLastLocation"]) == "0")
                                        {
                                            <option selected value="0">
                                                All Last Location
                                            </option>
                                        }
                                        else
                                        {
                                            <option value="0">
                                                All  Last Location
                                            </option>
                                        }
                                        @foreach (var item in ViewBag.LastLocation)
                                        {
                                            if (!string.IsNullOrEmpty(item.LastLocation))
                                            {
                                                if (item.LastLocation == Convert.ToString(Session["TestLastLocation"]) && Convert.ToString(Session["LastLocation"]) != "0"
                                                    && Convert.ToString(Session["TestLastLocation"]) != "")
                                                {
                                                    <option selected value="@item.LastLocation">@item.LastLocation</option>
                                                }
                                                else
                                                {
                                                    <option value="@item.LastLocation">@item.LastLocation</option>
                                                }
                                            }
                                        }
                                    </select>
                                </div>
                            </div>

                            <div class="col-md-1">
                                @*<div class="row_bar">
                                    <button id="btnSearch" type="submit" class="btn btn-primary btn-md" name="btnSearch">Search </button>
                                </div>*@
                                <div class="ds_filter_cont">

                                    <ul>
                                        <li><button id="btnSearch"  type="submit" data-toggle="tooltip" title="Search"><img src="~/images/search-icon.png" alt="" /></button"></li>
                                        <li><button id="btnReset" type="button" data-toggle="tooltip" title="Reset Filters"><img src="~/images/refresh-icon.png" alt="" /></button></li>
                                    </ul>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </form>
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-md-6">
            <table width="100%" border="0" cellspacing="0" cellpadding="0">
                <tr>
