<script>
    function GetPage(pPageNo, searchfield, pageSize, SortColumn, frompager, IsTotalTestOrders, IsTotalTestOrdersLast24hours, IsPostive, IsNegative,
        IsPending, IsPostiveNotNotified, IsNegativeNotNotified, IsPendingGreaterthan2days,
        IsPositiveLast24hours, IsPositiveInPatientLast24hours, IsNegativeLast24hours, IsPostiveNotNotifiedLast24hours,
        IsInPatientNotNotifiedLast24hours, IsNegativeNotNotifiedLast24hours, IsPositiveAdmissions,
        IsPositiveDeaths, IsPositiveDischargesLast24hours, IsPositiveDeathLast24hours, fromfilter) {
        //$("#loader-wrapper").show();
        //$("#loader").show();
        //alert("hi");
        //var LastNameList = $("#LastNameList").val();
        //var CityList = $("#CityList").val();
        //var StateList = $("#StateList").val();
        //var ZipList = $("#ZipList").val();
        //var PhoneList = $("#PhoneList").val();
        //var Keyword = $("#Keyword").val();
        var pageSize = $("#tblPatientDetails_length").val();
        var dataActionUrl = '@Url.Action("MoreCOVIDDetails", "Home")';
        
        if (frompager == 'onclick')
        {
            //alert(frompager);
            frompager = true;
        }
        if (fromfilter == true) {
            $('#tblPatientDetails_length').val("10").attr("selected", "selected");
            $("#searchfield").val('');
            pageSize = 10;
        }
        
        $.ajax({
            url: dataActionUrl,
            data: {
                pageNumber: pPageNo, searchfield: searchfield, pageSize: pageSize, SortColumn: SortColumn, IsTotalTestOrders: IsTotalTestOrders,
                IsTotalTestOrdersLast24hours: IsTotalTestOrdersLast24hours, IsPostive: IsPostive, IsNegative: IsNegative,
                IsPending: IsPending,
                IsPostiveNotNotified: IsPostiveNotNotified, IsNegativeNotNotified: IsNegativeNotNotified,
                IsPendingGreaterthan2days: IsPendingGreaterthan2days,
                IsPositiveLast24hours: IsPositiveLast24hours, IsPositiveInPatientLast24hours: IsPositiveInPatientLast24hours,
                IsNegativeLast24hours: IsNegativeLast24hours, IsPostiveNotNotifiedLast24hours: IsPostiveNotNotifiedLast24hours,
                IsInPatientNotNotifiedLast24hours: IsInPatientNotNotifiedLast24hours,
                IsNegativeNotNotifiedLast24hours: IsNegativeNotNotifiedLast24hours,
                IsPositiveAdmissions: IsPositiveAdmissions, IsPositiveDeaths: IsPositiveDeaths, IsPositiveDischargesLast24hours: IsPositiveDischargesLast24hours,
                IsPositiveDeathLast24hours: IsPositiveDeathLast24hours, fromfilter: fromfilter, frompager: frompager
            },
            type: "POST",
            success: function (data) {
                $("#dvCustomerList").empty();
                $("#dvCustomerList").html(data);
                //$("#loader-wrapper1").hide();
                //$("#loader").hide();
            },
            error: function (data) {
                //$("#loader-wrapper1").hide();
                //$("#loader").hide();
            }
        });
    }
</script>